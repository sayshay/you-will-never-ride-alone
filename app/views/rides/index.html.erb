<div class="container">
  <div class="row">
    <div class="col-xs-3">
      <h3><%= @user.name %> Profile</h3>
      <h3>Create Ride</h3>
    </div>
    <div class="col-xs-9">
      <h2>List of Rides</h2>
      <ul>
        <%= @rides.each do |ride| %>
        <li>ride.starting_city</li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
